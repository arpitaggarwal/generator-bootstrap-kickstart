{
  "name": "<%= _.slugify(projectName) %>",
  "description": "<%= projectDescription %>",
  "version": "<%= initialVersion %>",
  "authors": [
    "<%= authorName %> <<%= authorMail %>>"
  ],
  "license": "<%= license %>",
  "homepage": "<%= projectHomepage %>",
  "repository": {
    "type": "<%= projectRepositoryType %>",
    "url": "<%= projectRepository %>"
  },
  "keywords": [
    "bootstrap",
    "boilerplate",
    "skeleton",
    "scaffolding",
    "scaffold",
    "theme",
    "themes",
    "kickstart",
    "kickstarter",
    "micromata",
    "workflow"
  ],
  "dependencies": {
    "bootstrap": "~3.3.5"<% if (oldIeSupport) { %>,
    "jquery": "^1.11.3",
    "html5shiv": "^3.7.3",
    "respondJs": "~1.4.2",
    "jquery-placeholder": "~2.1.2"<% } else { %>,
    "jquery": "^2.1.4"<% } %>

  }<% if (oldIeSupport) { %>,
  "resolutions": {
    "jquery": "1.11.3"
  }<% } %>
}
